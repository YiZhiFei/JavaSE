package com.njxxyizhifei.javase.day10.test006;

public class Test {


		
		//带有static的方法
		public static void method1(){
			
		}
		
		//没有static的方法
		public void method2(){
			
		}

		//主方法
		public static void main(String[] args) {
		//没有static的变量
		int i = 10;
		
		//带有static的方法
		public static void doSome(){
			System.out.println("do Some!");
		}
		
		//没有static的方法
		public void doOther(){
			System.out.println("do Other!");
		}

	}

}
