package com.njxxyizhifei.javase.day09.test002;

public class UserTest {
	/**
	 * 用户测试类
	 * 
	 * @param args
	 */
	public static void main(String[] args) {

		// 创建User对象
		User user = new User();

		// 访问age
		// 读取年龄值
		System.out.println("该用户年龄：" + user.age);

		// 修改年龄值【set】
		user.age = 20;
		
		// 读取年龄【get】
		System.out.println("该用户年龄：" + user.age);
		
		// 修改年龄值
		user.name = -100;
		
		// 读取年龄值
		System.out.println("该用户年龄：" + user.age);

	}

}
